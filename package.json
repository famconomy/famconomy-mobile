{
  "name": "famconomy-mobile",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "build:ios": "react-native build-ios",
    "build:android": "react-native build-android",
    "fix-boost": "sed -i '' 's|https://boostorg.jfrog.io/artifactory/main/release|https://archives.boost.io/release|g' node_modules/react-native/third-party-podspecs/boost.podspec",
    "pod-install": "npm run fix-boost && rm -rf ~/Library/Caches/CocoaPods/Pods/Release/boost && cd ios && pod install",
    "postinstall": "npm run build:shared",
    "build:shared": "cd packages/shared && npm run build"
  },
  "dependencies": {
    "@famconomy/shared": "file:./packages/shared",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native/metro-config": "^0.80.2",
    "@react-navigation/native": "^6.1.18",
    "@supabase/supabase-js": "^2.90.1",
    "react": "18.2.0",
    "react-native": "0.72.4",
    "react-native-biometrics": "^3.0.1",
    "react-native-device-info": "^10.12.0",
    "react-native-gesture-handler": "^2.29.0",
    "react-native-haptic-feedback": "^2.2.0",
    "react-native-keychain": "^8.2.0",
    "react-native-permissions": "^4.1.5",
    "react-native-reanimated": "3.3.0",
    "react-native-safe-area-context": "^4.14.1",
    "react-native-screens": "^3.37.0",
    "react-native-svg": "^15.15.1",
    "react-native-svg-transformer": "^1.5.2",
    "react-native-webview": "^13.8.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@types/react": "18.3.5",
    "@types/react-native": "0.72.4",
    "typescript": "^5.3.0"
  }
}
